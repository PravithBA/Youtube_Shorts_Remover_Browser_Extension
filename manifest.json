{
    "name": "Youtube Shorts Remover",
    "description": "Removes youtube shorts from your feed",
    "version": "0.0.1",
    "manifest_version": 3,
        "content_scripts" :[
        {
            "matches" : ["https://*.youtube.com/*","http://*.youtube.com/*"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
        "tabs"
    ]
}