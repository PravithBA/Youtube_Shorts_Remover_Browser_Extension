{
    "name": "Youtube Shorts Remover",
    "description": "Removes youtube shorts from your feed",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts" :[
        {
            "matches" : ["https://*.youtube.com/*","http://*.youtube.com/*"],
            "js": ["index.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage",
        "declarativeContent",
        "activeTab",
        "scripting",
        "tabs",
        "notifications"],
    "host_permissions": [
        "https://*.youtube.com/*",
        "http://*.youtube.com/*",
        "http://*/",
        "https://*/",
        "devtools://devtools/*"
    ]
}